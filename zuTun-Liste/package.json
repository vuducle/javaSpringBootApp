{
  "name": "zu-tun-liste",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "generate-env": "node scripts/generate-env.js",
    "start": "npm run generate-env && ng serve",
    "build": "npm run generate-env && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:zuTun-Liste": "node dist/zuTun-Liste/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
